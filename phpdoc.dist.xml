<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>Project API Documentation</title>
    <parser>
        <target>web/docs</target>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
        <extensions>
            <extension>php</extension>
        </extensions>
        <visibility></visibility>
    </parser>
    <transformer>
        <target>web/docs</target>
    </transformer>
    <logging>
        <level>warn</level>
        <paths>
            <default>app/logs/phpdoc.log</default>
            <errors>app/logs/phpdoc.errors.log</errors>
        </paths>
    </logging>
    <transformations>
        <template name="responsive" />
    </transformations>
    <files>
        <directory>src</directory>
        <directory>vendor</directory>
    </files>
</phpdoc>
